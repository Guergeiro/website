#!/bin/bash

MODULE="$1"
VERSION="$2"

echo -e "Building version files for $MODULE@$VERSION\n"

scripts/compile_vue_routes $MODULE

node_modules/.bin/webpack \
  --config ./webpack.config.version.js \
  --hide-modules \
  --env.environment=version \
  --env.module=$MODULE \
  --env.version=$VERSION
